{
  "include": [
    "src/**/*.tsx",
    "src/**/*.ts",
    "src/**/*.jsx",
    "src/**/*.js",
    "src/**/*.svg",
    "src/**/*.png",
    "src/**/*.jpg",
    "src/**/*.gif"
  ],
  "compilerOptions": {
    "outDir": "./build/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "strict": true,
    "baseUrl": "cypress"
  }
}